<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.herodoc.study">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".loginAct" />
        <activity android:name=".registerAct1" />
        <activity android:name=".registerAct2" />
        <activity android:name=".registerAct3" />
        <activity android:name=".indexAct" />
        <activity android:name=".welcomeAct1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".indexAct1" />
        <activity
            android:name=".vendorAct"
            android:label="@string/title_activity_vendor"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>