<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.herodoc.study"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.herodoc.study.MainApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.herodoc.study.loginAct" />
        <activity android:name="com.example.herodoc.study.registerAct1" />
        <activity android:name="com.example.herodoc.study.registerAct2" />
        <activity android:name="com.example.herodoc.study.registerAct3" />
        <activity android:name="com.example.herodoc.study.indexAct" />
        <activity android:name="com.example.herodoc.study.welcomeAct1" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.herodoc.study.indexAct1" />
        <activity
            android:name="com.example.herodoc.study.vendorAct"
            android:label="@string/title_activity_vendor"
            android:theme="@style/AppTheme" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.herodoc.study.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>